
public class ProducerConsumer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new Thread(new Consumer("c1", new Person("c1"))).start();
		new Thread(new Consumer("c2", new Person("c2"))).start();
		new Thread(new Consumer("c3", new Person("c3"))).start();
		new Thread(new Consumer("c4", new Person("c4"))).start();
		new Thread(new Consumer("c5", new Person("c5"))).start();
		new Thread(new Consumer("p1", new Person("p1"))).start();
		new Thread(new Consumer("p2", new Person("p2"))).start();
		new Thread(new Consumer("p3", new Person("p3"))).start();
		new Thread(new Consumer("p4", new Person("p4"))).start();
		new Thread(new Consumer("p5", new Person("p5"))).start();
		new Thread(new Consumer("p6", new Person("p6"))).start();
	}

}


