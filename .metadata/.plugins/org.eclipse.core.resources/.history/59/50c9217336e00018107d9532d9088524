class MyThreadInterface implements Runnable {
	private int count = 0;
	private int time;
	public MyThreadInterface(int time){
		this.time = time;
	}
	public void run(){
		Synchronized(this)
		{
			while(true){
			try {
				Thread.sleep(time);
				System.out.println("Thread:" + Thread.currentThread().getName() + " "+ count);
				count++;
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
			}
	}
}
