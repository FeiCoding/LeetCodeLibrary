class MyThreadInterface implements Runnable {
	private String name;
	private int count = 0;
	private int time;
	public MyThreadInterface(String name, int time){
		this.name = name;
		this.time = time;
	}
	public void run(){
		while(true){
			try {
				Thread.sleep(time);
				System.out.println("Thread:" + name + " "+ count);
				count++;
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
